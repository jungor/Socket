CC=g++
FLAG=-g -std=c++11
ALL: Cmain AgendaUI Storage User Meeting Date
	$(CC) $(FLAG) -o client *.o
Cmain: Cmain.cpp
	$(CC) $(FLAG) -c Cmain.cpp
AgendaUI: AgendaUI.cpp
	$(CC) $(FLAG) -c AgendaUI.cpp
Storage: Storage.cpp
	$(CC) $(FLAG) -c Storage.cpp
User: User.cpp
	$(CC) $(FLAG) -c User.cpp
Meeting: Meeting.cpp
	$(CC) $(FLAG) -c Meeting.cpp
Date: Date.cpp
	$(CC) $(FLAG) -c Date.cpp
clean:
	rm *.o client
